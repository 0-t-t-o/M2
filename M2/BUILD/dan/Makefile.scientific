# -*- makefile-gmake -*-
DIRECTORY = production
ifeq ($(DEBUG),yes)
   CONFIGOPTIONS += --enable-debug --enable-memdebug --disable-optimize
   DIRECTORY := debug
else
   CONFIGOPTIONS += --enable-rpm
endif
ifeq ($(NTL_WIZARD),no)
   CONFIGOPTIONS=--disable-ntl-wizard 
endif
include Makefile.include
default: check

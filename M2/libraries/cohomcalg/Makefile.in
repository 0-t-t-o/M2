URL = http://macaulay2.com/Downloads/OtherSourceCode
VERSION = 0.32
TARFILE = cohomCalg-$(VERSION).tar.gz
LICENSEFILES = LICENSE
CHECKTARGET = .
CONFIGURECMD = :
INSTALLCMD = :
PROGRAMS = bin/cohomcalg
STRIPFILES = $(PROGRAMS)
